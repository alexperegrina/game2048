package fib.as.game2048;

public class Jugador extends UsuariRegistrat {
	
	private String email;
	private Integer millorPuntuacio;
	
	public Jugador()
	{
		super();
	}
	
	public Jugador(String nom, String cognoms, String username, String pwd, String email, Integer millorPuntuacio)
	{
		super(nom,cognoms,username,pwd);
		this.email = email;
		this.millorPuntuacio = millorPuntuacio;
	}
	
}
